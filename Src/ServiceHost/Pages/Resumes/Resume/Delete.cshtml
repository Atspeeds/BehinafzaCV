@model PersonalInformationManagement.Application.Contract.ResumeCon.Resume_Edit_Request


<div class="modal-header" style="direction:initial">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
    <h4 class="modal-title">ایجاد رزومه جدید</h4>
</div>

<form asp-page="Index" asp-page-handler="Delete"
      method="post"
      data-ajax="true"
      data-callback=""
      data-action="Refresh"
      enctype="multipart/form-data">
    <div class="modal-body">
        <div class="row">
            <div class="col-md-12">
                <div class="form-group">
                    آیا از حذف رزومه با عنوان (@Model.Summary) اطمینان داری.
                </div>
            </div>
            <input type="hidden" asp-for="Id" value="@Model.Id" />
        </div>

    </div>
    <div class="modal-footer">
        <button type="button" class="btn btn-default waves-effect" data-dismiss="modal">بستن</button>
        <button type="submit" class="btn btn-danger waves-effect waves-light">حذف</button>
    </div>
</form>